@page
@using crossPublisher
@model IndexModel

@{
    Layout = ViewData["Layout"]?.ToString();

    Repository currPage = ViewData["currPage"] as Repository;
    string lang = ViewData["currLang"]?.ToString();
    string error = ViewData["error"]?.ToString();
}

<h1>@currPage.GetTitle(lang)</h1>

<p>@error</p>

<p>lang: @lang</p>
<p>RepositoryID: @currPage.RepositoryID</p>